<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">
    <Button
        android:id="@+id/keySearchBT"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="关键字搜索/联想搜索"/>
    <Button
        android:id="@+id/resgeoBT"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="逆地理(24.4363,118.1025)"
        android:layout_marginTop="10dp"/>
    <Button
        android:id="@+id/geoBT"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="地理编码(厦门大学)"
        android:layout_marginTop="10dp"/>

    <Button
        android:id="@+id/searchByLocation"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="经纬度获取行政区(118.1025,24.4363)"
        android:layout_marginTop="10dp"/>

    <Button
        android:id="@+id/searchByKeyword"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="获取行政区数据(厦门市)"
        android:layout_marginTop="10dp"/>

    <Button
        android:id="@+id/searchByLocation4Town"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="获取4级行政区数据(山西：a{veFw|mmT)"
        android:layout_marginTop="10dp"/>


    <Button
        android:id="@+id/geoconvService"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="在线坐标转换"
        android:layout_marginTop="10dp"/>

</LinearLayout>